<?xml version="0.0" ?>
<robot name="kitchen.urdf">
  <link name="baseLink">
    <contact>
      <lateral_friction value="1.0"/>
    </contact>

    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
       <mass value=".0"/>
       <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
    </inertial>

    <visual>
      <origin rpy="1.57075 0 0" xyz="0 -1 0"/>
      <geometry>
        <mesh filename="kitchen.obj" scale="1 1 1"/>
      </geometry>
       <material name="gray">
        <color rgba="0.8 0.8 0.8 1"/>
      </material>
    </visual>

    <collision concave="yes">
      <origin rpy="1.57075 0 0" xyz="0 -1 0"/>
      <geometry>
	 	    <mesh filename="kitchen.obj" scale="1 1 1"/>
      </geometry>
    </collision>
  </link>
</robot>

